{
  "name": "ym-electron",
  "version": "1.0.15",
  "description": "ym electron",
  "scripts": {
    "build": "rollup -c rollup.config.js && node scripts/build.js",
    "build:watch": "rollup -c rollup.config.js -w",
    "fix:prettier": "prettier \"./src\" --write",
    "test:prettier": "prettier \"./src\" --list-different"
  },
  "keywords": [
    "electron"
  ],
  "author": "youliso",
  "repository": {
    "type": "git",
    "url": "https://github.com/youliso/electron-modules"
  },
  "engines": {
    "electron": ">=13",
    "node": ">=16.14.0"
  },
  "files": [
    "**/*"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "electron": "^18.1.0",
    "electron-updater": "^5.0.1",
    "prettier": "^2.3.1",
    "rollup": "^2.51.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "^4.3.2"
  }
}
