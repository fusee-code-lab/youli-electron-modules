{
  "name": "@youliso/electronic",
  "version": "0.0.1",
  "description": "electron modules",
  "scripts": {
    "build": "node --max_old_space_size=8000 build.js",
    "build:watch": "rollup -c rollup.config.js -w",
    "fix:prettier": "prettier \"./src\" --write --config .prettierrc",
    "test:prettier": "prettier \"./src\" --list-different --config .prettierrc",
    "test": " npx electron ./test/main.js"
  },
  "keywords": [
    "electron",
    "electron-updater"
  ],
  "author": "youliso",
  "homepage": "https://youliso.github.io/electronic/",
  "repository": {
    "type": "git",
    "url": "https://github.com/youliso/electronic"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "electron": ">=20",
    "node": ">=18.12.0"
  },
  "files": [
    "**/*"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.4",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "electron": "^22.0.0",
    "electron-updater": "^5.3.0",
    "prettier": "^2.8.1",
    "rollup": "^3.7.0",
    "rollup-plugin-esbuild-minify": "^1.0.7",
    "rollup-plugin-typescript2": "^0.34.1",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.4"
  }
}
