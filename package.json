{
  "name": "@youliso/electron-modules",
  "version": "1.2.22",
  "description": "electron modules",
  "scripts": {
    "build": "rollup -c rollup.config.js && node build.js",
    "build:watch": "rollup -c rollup.config.js -w",
    "fix:prettier": "prettier \"./src\" --write --config .prettierrc",
    "test:prettier": "prettier \"./src\" --list-different --config .prettierrc",
    "test": " npx electron ./test/main.js"
  },
  "keywords": [
    "electron",
    "electron-updater"
  ],
  "author": "youliso",
  "homepage": "https://youliso.github.io/electron-modules/",
  "repository": {
    "type": "git",
    "url": "https://github.com/youliso/electron-modules"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "electron": ">=17",
    "node": ">=16.14.0"
  },
  "files": [
    "**/*"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "electron": "^20.0.3",
    "electron-updater": "^5.2.1",
    "prettier": "^2.7.1",
    "rollup": "^2.78.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4"
  }
}
