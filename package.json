{
  "name": "@youliso/electronic",
  "version": "1.0.8",
  "description": "electron modules",
  "scripts": {
    "build": "node --max_old_space_size=8000 build.js",
    "build:watch": "rollup -c rollup.config.js -w",
    "fix:prettier": "prettier \"./src\" --write --config .prettierrc",
    "test:prettier": "prettier \"./src\" --list-different --config .prettierrc",
    "test": " npx electron ./test/main.js"
  },
  "main": "index.js",
  "types": "mian/index.d.ts",
  "keywords": [
    "electron",
    "electron-updater"
  ],
  "author": "youliso",
  "repository": {
    "type": "git",
    "url": "https://github.com/youliso/electronic"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "electron": ">=19",
    "node": ">=16"
  },
  "files": [
    "**/*"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "electron": "^25.3.1",
    "electron-updater": "^6.1.1",
    "prettier": "^3.0.0",
    "rollup": "^3.26.3",
    "rollup-plugin-esbuild-minify": "^1.1.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "typescript": "^5.1.6"
  }
}
